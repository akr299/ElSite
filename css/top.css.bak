/* 採用スケジュール */
.flow_bar {
    display: flex;
    background-color:#FFFD55;
    }

    .folw_box {
        position: relative;
      display: inline-block;
      padding-left: 12px;
      color: #333;
      text-decoration: none;
      margin :50px;
    }
    .folw_box::before{
      content: '';
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 10px 10px 10px 10px;
    border-color: transparent transparent transparent #333;
    position: absolute;
    top: 40%;
    margin-top: auto;
    margin-left: 120%;
    }

    .round-content{
      padding-left: 25%;
    }

    .a{
        background-color:#FFFBCF;
        height: 150px;
        width: 150px;
        border-radius: 50%;
    }

    .folw_box:last-child::before{
      display: none;
    }

