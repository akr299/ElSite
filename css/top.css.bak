/* ヘッダー */

body {}

.header {
    height: 80px;
    display: flex;
    /* 縦方向の並び順*/
    /*
        flex-start：上揃え（デフォルト）
        flex-end：下揃え
        center：中央揃え
        baseline：ベースライン
        stretch：伸縮
        */
    align-items: center;
}

@import url('https://fonts.googleapis.com/css2?family=Sacramento&display=swap');

.logo {}

.logo_text {
    font-family: 'Sacramento', cursive;
    font-size: 50px;
}

.menu {
    margin-left: auto;
}

ul.navi {
    list-style: none;
    display: flex;
}

li.item {
    width: 140px;
    text-align: center;

}

li.item>a {
    text-decoration: none;
    color: dimgray;
}

/* /ヘッダー */

/* top */
.top_area {
    position: relative;
    padding: 0;
}

.top_img {
    width: 100%;
}

.top_text {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

/* /top */

/* logo_bar */
.wrap_logo_bar {
    display: flex;
    align-items: center;
    height: 200px;
    overflow: hidden;
}

.slideshow_logo {
    display: flex;
    animation: loop-slide 20s infinite linear 1s both;
}

@keyframes loop-slide {
    from {
        transform: translateX(0);
    }

    to {
        transform: translateX(-100%);
    }
}

.content {
    width: 200px;
    height: 160px;
}

.content:nth-child(1) {
    background-color: tomato;
}

.content:nth-child(2) {
    background-color: orange;
}

.content:nth-child(3) {
    background-color: blue;
}

.content:nth-child(4) {
    background-color: green;
}

/* /logo_bar */

